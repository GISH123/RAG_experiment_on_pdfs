{
    "version": 2,
    "builds": [
      {
        "src": "Step3_query_engine_as_server.py",
        "use": "@vercel/python",
        "config": {
          "pythonVersion": "3.10"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "Step3_query_engine_as_server.py"
      }
    ],
    "build": {
      "env": {
        "PYTHON_VERSION": "3.10"
      }
    }
  }